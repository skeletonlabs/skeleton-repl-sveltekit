		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "6.1.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/vite": "4.1.11",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"svelte": "5.21.0",
		"svelte-check": "4.3.0",
		"tailwindcss": "4.1.11",
		"typescript": "5.7.3",
		"vite": "^7.0.6"
	}
